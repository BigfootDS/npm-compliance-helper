{
  "name": "npm-compliance-helper",
  "version": "1.0.0",
  "description": "Tool to help generate an NPM dependency \"bill of materials\" and confirm whether your dependencies are commercially-usable or not.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "licensecheck:summary": "license-checker --summary",
    "licensecheck:filereport": "mkdir -p ./out && license-checker --json >> ./out/licenseReport.json",
    "licenseGenny": "generate-license-file --input ./package.json --output thirdpartylicenses --ci --overwrite"
  },
  "keywords": [],
  "author": {
    "name": "BigfootDS",
    "email": "contact@bigfootds.com",
    "url": "https://bigfootds.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigfootDS/npm-compliance-helper.git"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "generate-license-file": "^3.4.0"
  }
}
